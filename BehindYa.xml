<Interface xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Interface.xsd">
<Windows>
	<Window name="BehindYa" layer="secondary" popable="false" movable="false" savesettings="true" handleinput="false">
		<Size>
			<AbsPoint x="32" y="32" />
		</Size>
		<Anchors>
			<Anchor point="center" relativePoint="center" relativeTo="Root">
			<AbsPoint x="0" y="150" />
			</Anchor>
		</Anchors>
		<Windows>

			<Label name="$parentShadow" font="font_clear_medium_bold" autoresize="true" textalign="center" layer="popup" popable="false" handleinput="false">
				<Size>
					<AbsPoint x="64" y="32" />
				</Size>
				<Anchors>
					<Anchor point="center" relativePoint="center"  relativeTo="$parentLabel">
						<AbsPoint x="2" y="2" />
					</Anchor>
				</Anchors>
				<Color r="0" g="0" b="0" a="255" />
			</Label>
			<Label name="$parentLabel" font="font_clear_medium_bold" autoresize="true" textalign="center" layer="popup" popable="false" handleinput="false">
				<Size>
					<AbsPoint x="64" y="32" />
				</Size>
				<Anchors>
					<Anchor point="bottom" relativePoint="top"  relativeTo="$parentSpellIcon">
					<AbsPoint x="0" y="2" />
					</Anchor>
				</Anchors>
				<Color r="255" g="255" b="255" a="255" />
			</Label>
			<DynamicImage name="$parentSpellIcon">
				<Anchors>
					<Anchor point="center" relativePoint="center" relativeTo="BehindYa">
						<AbsPoint x="1" y="1" />
					</Anchor>
				</Anchors>
				<TexDims x="32" y="32" />
				<Size>
					<AbsPoint x="28" y="28" />
				</Size>
			</DynamicImage>
		</Windows>
	</Window>
</Windows>
</Interface>
